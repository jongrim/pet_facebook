<div class="container">
  <div class="page-header text-center">
    <h1>Profile Page</h1>
    <a href="/logout" class="btn btn-default btn-small">Logout</a>
  </div>
  <div class="row">
    <div class="col-6">
      <div class="well">
        <h3><span class="fa fa-user"></span> {{user.email}}</h3>
        <p><strong>id:</strong> {{user.id}}</p>
        <p><strong>email:</strong> {{user.email}}</p>
        <p><strong>password:</strong> {{user.password}}</p>
      </div>
    </div>
  </div>
</div>
